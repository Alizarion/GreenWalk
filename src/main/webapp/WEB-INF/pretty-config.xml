<pretty-config xmlns="http://ocpsoft.com/prettyfaces/3.3.2"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://ocpsoft.com/prettyfaces/3.3.2
               http://ocpsoft.com/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.3.2.xsd">

    <url-mapping id="comingsoon">
        <pattern value="/GreenWalk-coming-soon"/>
        <view-id>/comingsoon.jsf</view-id>
    </url-mapping>

    <url-mapping id="comingsoon">
        <pattern value="/profile/#{username}"/>
        <view-id>/profile.jsf</view-id>
    </url-mapping>

    <url-mapping id="signeEventComments">
        <pattern value="/picked-up/#{eventId}"/>
        <view-id>/singleEventDisplay.jsf</view-id>
    </url-mapping>


    <url-mapping id="groupEventComments">
        <pattern value="/group-pick-up/#{eventId}"/>
        <view-id>/groupEventDisplay.jsf</view-id>
    </url-mapping>


    <url-mapping id="privateConversation">
        <pattern value="/user/conversation/#{eventId}"/>
        <view-id>/user/privateConversation.jsf</view-id>
    </url-mapping>

    <url-mapping id="groupEventEdit">
        <pattern value="/user/group-pick-up-edit/#{eventId}"/>
        <view-id>/user/groupEvent.jsf</view-id>
    </url-mapping>




</pretty-config>